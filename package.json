{
  "name": "@untemps/react-vocal-demo",
  "version": "0.0.0",
  "author": "Vincent Le Badezet <v.lebadezet@untemps.net>",
  "repository": "git@github.com:untemps/react-vocal.git",
  "license": "MIT",
  "description": "React component and hook to initiate a SpeechRecognition session",
  "keywords": [
    "Web Speech API",
    "SpeechRecognition",
    "speech",
    "speech-to-text",
    "React",
    "ReactJS",
    "component",
    "hook",
    "javascript"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/plugin-transform-react-jsx": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-babel": "^5.0.2",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@testing-library/dom": "^7.7.1",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "autoprefixer": "^9.8.5",
    "babel-jest": "^26.0.1",
    "cross-env": "^7.0.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "rollup": "^2.11.2",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "semantic-release": "^17.0.7"
  },
  "dependencies": {
    "@untemps/react-vocal": "^1.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tailwindcss": "^1.5.1"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "chore",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  },
  "scripts": {
    "test": "jest -u --watch",
    "test:ci": "CI=true jest -u -b --coverage",
    "start": "rollup -c -w --environment DEVELOPMENT",
    "build": "rollup -c --environment PRODUCTION",
    "prettier": "prettier \"*/**/*.js\" --ignore-path ./.prettierignore --write"
  }
}
